{
    "version": 2,
    "builds": [
        {
            "src": "nuxt.config.js",
            "use": "@altinget/vercel-builder",
            "config": {
                "images": {
                    "sizes": [
                        320,
                        640,
                        768,
                        1024,
                        1280,
                        1536,
                        1536
                    ],
                    "minimumCacheTTL": 60,
                    "formats": [
                        "image/webp",
                        "image/avif"
                    ]
                }
            }
        }
    ]
}